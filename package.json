{
  "name": "openmagic.errortracker.core",
  "version": "2.0.0-1",
  "description": "Core library for OpenMagic.ErrorTracker projects.",
  "main": "OpenMagic.ErrorTracker.Core.sln",
  "watch": {
    "test": {
      "patterns": [
        "source",
        "tests"
      ],
      "extensions": "cs,csproj",
      "quiet": "true"
    }
  },
  "scripts": {
    "publish": "echo Use publish.cmd or npm run publish-package && exit 1",
    "postinstall": "run-task npm_postinstall",
    "test": "run-task test",
    "preversion": "run-task npm_preversion",
    "version": "run-task npm_version",
    "postversion": "run-task npm_postversion -- skipTests",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimMurphy/OpenMagic.ErrorTracker.Core.git"
  },
  "keywords": [
    "error",
    "tracker",
    "tracking"
  ],
  "author": "Tim Murphy <tim@26tp.com> (https://timmurphy.it)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TimMurphy/OpenMagic.ErrorTracker.Core/issues"
  },
  "homepage": "https://github.com/TimMurphy/OpenMagic.ErrorTracker.Core#readme",
  "devDependencies": {
    "del": "^2.2.0",
    "mkdirp": "^0.5.1",
    "npm-msbuild": "git+https://github.com/TimMurphy/npm-msbuild.git#v3.0.3",
    "npm-nuget": "git+https://github.com/TimMurphy/npm-nuget.git#v3.1.3",
    "npm-watch": "^0.1.4",
    "replace": "^0.3.0"
  },
  "private": true
}
